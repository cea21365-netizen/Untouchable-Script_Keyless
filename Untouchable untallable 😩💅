	while true do
		-- Original Rainbow for main title
		TitleText.TextColor3 = Color3.fromHSV((tick()*0.7)%1,1,1)
		
		-- Slow Red to Orange transition for "Friendshipcreate"
		local lerpVal = (math.sin(tick() * 1.5) + 1) / 2 -- Cycles slowly between 0 and 1
		FriendshipLabel.TextColor3 = Color3.fromRGB(255, 0, 0):Lerp(Color3.fromRGB(255, 120, 0), lerpVal)
		
		task.wait(0.03)
	end
end)

