-- =========================
-- HALO ENGINE
-- =========================
local function createHalo(character)
	local head = character:WaitForChild("Head")
	for _, v in ipairs(workspace.Terrain:GetChildren()) do
		if v.Name == "HaloFolder" then v:Destroy() end
	end
	local folder = Instance.new("Folder")
	folder.Name = "HaloFolder"
	folder.Parent = workspace.Terrain
	local segments = {}
	local angle = 0
	local currentSpeed = IDLE_SPEED
	local pulseTime = 0
	local wasAlert = false

	for i = 1, SEGMENTS do
		local a0, a1 = Instance.new("Attachment"), Instance.new("Attachment")
		a0.Parent, a1.Parent = folder, folder
		local trail = Instance.new("Trail")
		trail.Attachment0, trail.Attachment1 = a0, a1
		trail.Lifetime, trail.FaceCamera, trail.Enabled = 0.12, true, true
		trail.Parent = folder
		table.insert(segments, {a0 = a0, a1 = a1, trail = trail, offset = (i / SEGMENTS) * math.pi * 2})
	end

	local startTime = tick()
	RunService.Heartbeat:Connect(function(dt)
		if not Enabled then
			for _, seg in ipairs(segments) do seg.trail.Enabled = false end
			return
		else
			for _, seg in ipairs(segments) do seg.trail.Enabled = true end
		end
		if not character.Parent then return end

		local alert = false
		for _, plr in ipairs(Players:GetPlayers()) do
			if plr ~= LocalPlayer and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
				if (plr.Character.HumanoidRootPart.Position - head.Position).Magnitude 
