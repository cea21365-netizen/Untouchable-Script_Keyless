-- DRAGGING
local dragging, dragInput, dragStart, startPos
Frame.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = Frame.Position
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then dragging = false end
		end)
	end
end)
Frame.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		dragInput = input
	end
end)
UserInputService.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		local delta = input.Position - dragStart
		Frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
end)

MinBtn.MouseButton1Click:Connect(function()
	Minimized = not Minimized
	Content.Visible = not Minimized
	Frame.Size = Minimized and UDim2.fromOffset(280,30) or UDim2.fromOffset(280,460)
end)

Toggle.MouseButton1Click:Connect(function()
	Enabled = not Enabled
	Toggle.Text = Enabled and "Glide: ON" or "Glide: OFF"
	Toggle.BackgroundColor3 = Enabled and Color3.fromRGB(60,140,60) or Color3.fromRGB(140,50,50)
end)

ModeButton.MouseButton1Click:Connect(function()
	AvoidAll = not AvoidAll
	ModeButton.Text = AvoidAll and "Mode: ALL" or "Mode: SINGLE"
end)

RadiusMinus.MouseButton1Click:Connect(function()
	GlideRadius = math.max(1, GlideRadius-1); RadiusLabel.Text = "Radius: "..GlideRadius
end)
RadiusPlus.MouseButton1Click:Connect(function()
	GlideRadius += 1; RadiusLabel.Text = "Radius: "..GlideRadius
end)
SpeedMinus.MouseButton1Click:Connect(function()
	GlideSpeed = math.max(1,GlideSpeed-10); SpeedLabel.Text = "Speed: "..GlideSpeed
end)
SpeedPlus.MouseButton1Click:Connect(function()
	GlideSpeed += 10; SpeedLabel.Text = "Speed: "..GlideSpeed
end)

local function matchesPlayer(p)
	local txt = NameBox.Text or ""
	txt = txt:lower():gsub("%s","")
	if AvoidAll then return true end
	if txt == "" then return false end
	local uname = p.Name:lower():gsub("%s","")
	local dname = p.DisplayName:lower():gsub("%s","")
	return uname == txt or dname == txt or uname:find(txt,1,true) or dname:find(txt,1,true)
end

-- =========================
-- INTEGRATED TRAIL LOGIC
-- =========================
local trail, attachBack
local function setupTrail(character)
	local root = character:WaitForChild("HumanoidRootPart")
	for _, obj in ipairs(root:GetChildren()) do
		if obj:IsA("Trail") and obj.Name == "PushTrail" then obj:Destroy() end
	end
	local attachFront = Instance.new("Attachment")
	attachFront.Name = "TrailFront"; attachFront.Position = Vector3.new(0,0,0); attachFront.Parent = root
	local ab = Instance.new("Attachment")
	ab.Name = "TrailBack"; ab.Position = Vector3.new(0,0,-0.5); ab.Parent = root
	local t = Instance.new("Trail")
	t.Name = "PushTrail"; t.Attachment0 = attachFront; t.Attachment1 = ab
	t.Lifetime = TrailLifetimeValue; t.Enabled = false; t.MinLength = 0
	t.TextureMode = Enum.TextureMode.Stretch; t.FaceCamera = true
	t.Transparency = NumberSequence.new(0,1)
	t.WidthScale = NumberSequence.new({NumberSequenceKeypoint.new(0, 0.05), NumberSequenceKeypoint.new(1, 0.2)})
	t.Parent = root
	spawn(function()
		while t and t.Parent do
			for h=0,1,0.01 do t.Color = ColorSequence.new(Color3.fromHSV(h,1,1)); task.wait(0.02) end
		end
	end)
	trail = t; attachBack = ab
end

setupTrail(LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait())
LocalPlayer.CharacterAdded:Connect(setupTrail)

local function updateTrailSettings()
	local length = tonumber(TrailRadiusBox.Text)
	local duration = tonumber(TrailDurationBox.Text)
	if length then TrailLength = length end
	if duration then TrailLifetimeValue = duration; if trail then trail.Lifetime = duration end end
end
TrailRadiusBox.FocusLost:Connect(updateTrailSettings)
TrailDurationBox.FocusLost:Connect(updateTrailSettings)

-- MAIN LOOP
RunService.Heartbeat:Connect(function()
	local char = LocalPlayer.Character
	local root = char and char:FindFirstChild("HumanoidRootPart")
	if not root then return end

	local vel = root.AssemblyLinearVelocity
	if TrailEnabled and vel.Magnitude > 0.5 and trail then
		trail.Enabled = true
		attachBack.Position = -vel.Unit * TrailLength
	elseif trail then
		trail.Enabled = false
	end

	if not Enabled then return end

	local pushVector, count = Vector3.zero, 0
	for _, other in ipairs(Players:GetPlayers()) do
		if other == LocalPlayer or not other.Character then continue end
		if not matchesPlayer(other) then continue end
		local oRoot = other.Character:FindFirstChild("HumanoidRootPart")
		if not oRoot then continue end
		local offset = root.Position - oRoot.Position
		if offset.Magnitude <= GlideRadius then
			local dir = Vector3.new(offset.X,0,offset.Z)
			if dir.Magnitude>0 then pushVector += dir.Unit; count +=1 end
		end
	end
	if count>0 then
		root.AssemblyLinearVelocity = (pushVector/count).Unit * GlideSpeed + Vector3.new(0,UPWARD_LIFT,0)
	end
end)

